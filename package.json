{
  "name": "file-server",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "main": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazimanzurrashid/file-server"
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "format": "prettier --check --write ./**/*.{ts,js,json,md,yml,yaml}",
    "lint": "eslint ./ --fix",
    "pre-commit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --ci --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "release-it",
    "start": "nodemon src/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.289.0",
    "@azure/storage-blob": "^12.13.0",
    "@google-cloud/storage": "^6.9.4",
    "@redis/client": "^1.5.6",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "mongodb": "^5.1.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "parse-duration": "^1.0.3",
    "pino": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.2",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vercel/ncc": "^0.36.1",
    "copy-dir": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "release-it": "^15.10.0",
    "rimraf": "^4.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
