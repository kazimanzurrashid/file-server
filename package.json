{
  "name": "file-server",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "main": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazimanzurrashid/file-server"
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "format": "prettier --check --write ./**/*.{ts,js,json,md,yml,yaml}",
    "lint": "eslint ./ --fix",
    "pre-commit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --ci --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "release-it",
    "start": "nodemon src/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@azure/storage-blob": "^12.15.0",
    "@google-cloud/storage": "^7.0.1",
    "@redis/client": "^1.5.9",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "mongodb": "^5.8.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "parse-duration": "^1.1.0",
    "pino": "^8.15.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.7",
    "@types/node-cron": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/ncc": "^0.36.1",
    "copy-dir": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "release-it": "^16.1.5",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
