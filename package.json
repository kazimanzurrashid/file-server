{
  "name": "file-server",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "main": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazimanzurrashid/file-server"
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "format": "prettier --check --write ./**/*.{ts,js,json,md,yml,yaml}",
    "lint": "eslint ./ --fix",
    "pre-commit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --ci --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "release-it",
    "start": "nodemon src/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.588.0",
    "@azure/storage-blob": "^12.18.0",
    "@google-cloud/storage": "^7.11.2",
    "@redis/client": "^1.5.16",
    "express": "^4.19.2",
    "express-pino-logger": "^7.0.0",
    "mongodb": "^6.7.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "parse-duration": "^1.1.0",
    "pino": "^9.1.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vercel/ncc": "^0.38.1",
    "copy-dir": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-github": "^5.0.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.0",
    "release-it": "^17.3.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
