{
  "name": "file-server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "format": "prettier --write ./**/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --ci --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build ./build/src/index.js --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "npm run format && npm run lint && npm run coverage && npm run build && npm run pack && npm run copy-other-required-files"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@azure/storage-blob": "^12.11.0",
    "@google-cloud/storage": "^6.5.2",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "parse-duration": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.8.3",
    "@types/node-cron": "^3.0.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vercel/ncc": "^0.34.0",
    "copy-dir": "^1.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
