{
  "name": "file-server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"node_modules/.bin/ts-node src/index.ts",
    "format": "prettier --write ./**/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build ./build/src/index.js --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "npm run format && npm run lint && npm run coverage && npm run build && npm run pack && npm run copy-other-required-files"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.163.0",
    "@azure/storage-blob": "^12.11.0",
    "@google-cloud/storage": "^6.4.2",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "parse-duration": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "@types/node-cron": "^3.0.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vercel/ncc": "^0.34.0",
    "copy-dir": "^1.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
